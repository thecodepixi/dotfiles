[user]
	name = Emily Harber
	email = thecodepixi@users.noreply.github.com
[core]
	commitGraph = true
	editor = vim
[diff]
	algorithm = patience
[gc]
	writeCommitGraph = true
[protocol]
	version = 2
[push]
	default = current
[pull]
	rebase = true
	prune = true
[rerere]
	enabled = true
[fetch]
	prune = true
[url "https://github.com/Shopify/"]
	insteadOf = git@github.com:Shopify/
	insteadOf = git@github.com:shopify/
	insteadOf = ssh://git@github.com/Shopify/
	insteadOf = ssh://git@github.com/shopify/
[credential]
	helper = store --file /opt/dev/var/private/git_credential_store
	helper = /usr/local/share/gcm-core/git-credential-manager-core
[alias]
	s = status
	co = checkout
	cob = checkout -b
	p = push
	pfl = push --force-with-lease
	cm = commit -m
	d = diff
	r = rebase
	ri = rebase -i
	aa = add .
	a = add

[init]
	defaultBranch = main
[credential "https://dev.azure.com"]
	useHttpPath = true
